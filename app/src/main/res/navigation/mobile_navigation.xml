<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="edu.ort.pastillapp.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_symptoms"
        android:name="edu.ort.pastillapp.ui.symtoms_.SymtomsFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="edu.ort.pastillapp.ui.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_navigation_profile_to_contactRequests2"
            app:destination="@id/contactRequests2" />
    </fragment>
    <fragment
        android:id="@+id/navigation_registerPill"
        android:name="edu.ort.pastillapp.ui.register_pill.RegisterPillFragment"
        android:label="fragment_register_pill"
        tools:layout="@layout/fragment_register_pill" />
    <fragment
        android:id="@+id/navigation_calendar"
        android:name="edu.ort.pastillapp.ui.calendar_.CalendarFragment"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar" />
    <fragment
        android:id="@+id/contactRequests2"
        android:name="edu.ort.pastillapp.fragments.contactRequests"
        android:label="fragment_contact_requests"
        tools:layout="@layout/fragment_contact_requests" />

</navigation>